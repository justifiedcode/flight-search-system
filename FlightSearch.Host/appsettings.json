{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting": "Warning",
      "FlightSearch": "Information"
    }
  },
  "AllowedHosts": "*",
  "FlightProviders": {
    "Amadeus": {
      "Name": "Amadeus",
      "SimulationDelayMs": {
        "Min": 8000,
        "Max": 12000
      },
      "FailureRate": 0.05
    },
    "Skyscanner": {
      "Name": "Skyscanner",
      "SimulationDelayMs": {
        "Min": 15000,
        "Max": 20000
      },
      "FailureRate": 0.1
    },
    "Aviationstack": {
      "Name": "Aviationstack",
      "SimulationDelayMs": {
        "Min": 25000,
        "Max": 30000
      },
      "FailureRate": 0.05
    }
  },
  "SearchTimeout": {
    "TimeoutSeconds": 45
  }
}